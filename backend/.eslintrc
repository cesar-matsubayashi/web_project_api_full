{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "linebreak-style": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}
